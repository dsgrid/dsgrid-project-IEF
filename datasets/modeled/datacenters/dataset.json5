{
  dataset_id: "ief_2025_data_centers",
  dataset_type: "modeled",
  sector_description: "Commercial",
  description: "Data center load for the IEF 2023 High Scenario, with the projection based in part on EPRI (2024), 'Analyzing Artificial Intelligence and Data Center Energy Consumption'",
  origin_creator: "Ashreeta Prasanna",
  origin_organization: "NREL",
  origin_contributors: [
    "Ashreeta Prasanna",
    "Elaine Hale",
    "Sarah Smith"
  ],
  origin_project: "IEF Year 1, Task 6",
  origin_date: "2024-07-08",
  origin_version: "v0.1.0",
  data_source: "EPRI, LBL",
  source: "Datacenter load for IEF 2023",
  data_classification: "low",
  tags: [
    "datacenters",
    "data center"
  ],
  trivial_dimensions: [
    "scenario",
    "subsector",
    "weather_year",
  ],
  data_schema: {
    table_format: {
      format_type: "unpivoted",
      value_column: "value",
    },
    data_schema_type: "one_table",
  },

  dimensions: [
    {
      type: "geography",
      name: "state",
      file: "../../../datasets/historical/eia_861_annual_energy_use_state_sector/dimensions/states.csv",
      module: "dsgrid.dimension.standard",
      class: "State",
      description: "US States L48",
    },
    {
      type: "metric",
      name: "unspecified_electricity",
      file: "../../../datasets/modeled/dimensions/enduses.csv",
      module: "dsgrid.dimension.standard",
      class: "EnergyEndUse",
      description: "Unspecified end use electricity",
    },
    {
      type: "model_year",
      name: "model_year",
      file: "dimensions/model_years.csv",
      module: "dsgrid.dimension.standard",
      class: "ModelYear",
      description: "Projections of annual data center load 2023 through 2050",
    },
    {
      type: "scenario",
      class: "Scenario",
      name: "high_scenario",
      description: "IEF 2023 High-Resolution Demand Scenarios; Limited to a single, high scenario",
      file: "../../../project/dimensions/scenarios.csv",
      module: "dsgrid.dimension.standard",
    },
    {
      type: "sector",
      name: "sector",
      file: "dimensions/sectors.csv",
      module: "dsgrid.dimension.standard",
      class: "Sector",
      description: "Commercial Sector",
    },
    {
      type: "subsector",
      class: "Subsector",
      name: "datacenter",
      description: "data center subsector",
      file: "dimensions/subsectors.csv",
      module: "dsgrid.dimension.standard",
    },
    {
      type: 'time',
      class: 'AnnualTime',
      time_type: 'annual',
      module: 'dsgrid.dimension.standard',
      description: 'Annual time 2023-2050',
      name: 'annual',
      ranges: [
        {
          start: '2023',
          end: '2050',
        },
      ],
      str_format: '%Y',
      include_leap_day: true,
      measurement_type: 'total',
    },
    {
      type: "weather_year",
      class: "WeatherYear",
      name: "weather_2018",
      description: "Weather year, 2018 only",
      file: "../../../project/dimensions/weather_years.csv",
      module: "dsgrid.dimension.standard",
    },
  ],
}
