{
  dataset_id: "ief_2025_dgen",
  dataset_type: "modeled",
  data_source: "dgen",
  sector_description: "residential and commercial",
  description: "dGen Standard Scenarios 2023 with profiles using 2018 as the weather year - using same dataset as 2023",
  origin_creator: "Ashreeta Prasanna",
  origin_organization: "NREL",
  origin_contributors: [
    "Ashreeta Prasanna",
    "Paritosh Das",
    "Jane Lockshin",
    "Michaela Sizemore",
  ],
  origin_project: "Standard Scenarios 2023",
  data_source_date: "2024-11-01",
  data_source_version: "0.1.0",
  data_source_doi_url: "dGen runs for Standard Scenarios 2023",
  data_classification: "low",
  tags: [
    "distributed PV",
    "PV generation",
    "residential and commercial PV generation",
    "dGen",
    "Standard Scenarios 2023",
    "ief",
  ],
  trivial_dimensions: [
    "weather_year",
    "scenario",
  ],
  use_project_geography_time_zone: true,
  dimensions: [
    {
      "class": "GeographyDimensionBaseModel",
      type: "geography",
      name: "census_tract",
      file: "dimensions/geography.csv",
      module: "dsgrid.dimension.standard",
      description: "US tracts for L48, Census 2010 vintage mapped to 2020 counties, no territories",
    },
    {
      type: "sector",
      name: "sector",
      file: "dimensions/sectors.csv",
      module: "dsgrid.dimension.standard",
      "class": "Sector",
      description: "dGen models DPV adoption for the residential and non-residential sectors",
    },
    {
      "class": "Subsector",
      description: "tilt and azimuth of systems specified as subsectors",
      file: "dimensions/subsectors.csv",
      module: "dsgrid.dimension.standard",
      name: "tilt_azimuth_subsector",
      type: "subsector",
    },
    {
      type: "metric",
      name: "dpv_generation",
      file: "dimensions/enduses.csv",
      module: "dsgrid.dimension.standard",
      "class": "EnergyEndUse",
      description: "dGen DPV generation in kWh",
    },
    {
      type: "scenario",
      name: "dgen_stscen2022_low_re_cost",
      file: "dimensions/scenario.csv",
      module: "dsgrid.dimension.standard",
      "class": "Scenario",
      description: "ReEDS scenario High and Breakthrough mapped to dGen scenario StScen2022_Low_RE_Cost",
    },
    {
      type: "model_year",
      name: "dgen_model_year",
      file: "dimensions/model_years.csv",
      module: "dsgrid.dimension.standard",
      "class": "ModelYear",
      description: "dGen model years for Standard Scenarios, 2014-2050 mapped to project model years",
    },
    {
      "class": "WeatherYear",
      description: "Weather year, 2018 only",
      file: "dimensions/weather_year.csv",
      module: "dsgrid.dimension.standard",
      name: "weather_2018",
      type: "weather_year",
    },
    {
      type: "time",
      "class": "Time",
      frequency: "P0DT1H0M0.000000S",
      name: "time_utc",
      time_type: "datetime",
      leap_day_adjustment: "none",
      description: "Time dimension, 2018 hourly UTC, hour-beginning, no Leap Day Adjustment, total value",
      time_interval_type: "period_beginning",
      str_format: "%Y-%m-%d %H:%M:%S",
      timezone: "UTC",
      measurement_type: "total",
      ranges: [
        {
          start: "2018-01-01 00:00:00",
          end: "2018-12-31 23:00:00",
        },
      ],
    },
  ],
  data_layout: {
    table_format: "two_table",
    value_format: "pivoted",
    pivoted_dimension_type: "metric",
    data_file: {
      path: "load_data.parquet",
    },
    lookup_data_file: {
      path: "load_data_lookup.parquet",
    },
  },
}
