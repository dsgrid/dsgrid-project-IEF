{
  dataset_id: "ief_2025_buildings",
  dataset_type: "modeled",
  sector_description: "residential_and_commercial_building_stock",
  description: "High-resolution building end-use electricity load profiles for contiguous US for four scenarios, simulated using Scout with end use load profile data from ResStock and ComStock.",
  origin_creator: "Margaret Pigman",
  origin_organization: "LBL",
  origin_contributors: [
    "Jared Langevin",
    "Jeff Deason",
    "Chioke Harris",
    "Henry Horsey",
    "Andrew Parker",
    "Handi Chandra Putra",
  ],
  origin_project: "Buildings Standard Scenarios",
  origin_date: "2025-01-31",
  origin_version: "v1",
  data_source: "bss",
  source: "B-SS public data release, with data from ResStock (2024/resstock_amy2018_release_2) and ComStock (2025/comstock_amy2018_release_2)",
  data_classification: "low",
  tags: [
    "residential",
    "commercial",
    "buildings",
    "Scout",
    "ResStock",
    "ComStock"
  ],
  trivial_dimensions: [
    "subsector",
    "weather_year",
  ],
  data_schema: {
    table_format: {
      format_type: "pivoted",
      pivoted_dimension_type: "metric",
    },
    data_schema_type: "one_table",
  },

  dimensions: [
    {
      type: "geography",
      name: "county",
      file: "dimensions/county.csv",
      module: "dsgrid.dimension.standard",
      class: "GeographyDimensionBaseModel",
      description: "US counties for lower 48 states from 2015 census",
    },
    {
      type: "metric",
      name: "building_end_use",
      file: "dimensions/enduses.csv",
      module: "dsgrid.dimension.standard",
      class: "EnergyEndUse",
      description: "Scout residential and commercial end uses (N=9) based on EIA Annual Energy Outlook",
    },
    {
      type: "model_year",
      name: "building_model_year",
      file: "dimensions/model_years.csv",
      module: "dsgrid.dimension.standard",
      class: "ModelYear",
      description: "2024 and every 5 years from 2025 through 2050",
    },
    {
      type: "scenario",
      class: "Scenario",
      name: "building_scenario",
      description: "2025 High-Resolution Demand Scenarios",
      file: "dimensions/scenarios.csv",
      module: "dsgrid.dimension.standard",
    },
    {
      type: "sector",
      name: "building_sector",
      file: "dimensions/sectors.csv",
      module: "dsgrid.dimension.standard",
      class: "Sector",
      description: "Building Sectors",
    },
    {
      type: "subsector",
      class: "Subsector",
      name: "unspecified_subsector",
      description: "Unspecified subsectors",
      file: "dimensions/subsectors.csv",
      module: "dsgrid.dimension.standard",
    },
    {
      type: "time",
      class: "Time",
      frequency: "P0DT1H0M0.000000S",
      name: "time_est",
      time_type: "datetime",
      leap_day_adjustment: "none",
      description: "Time dimension, 2018 hourly EST, period ending, no Leap Day Adjustment, total value",
      time_interval_type: "period_ending",
      str_format: "%Y-%m-%d %H:%M:%S",
      timezone: "EasternStandard",
      measurement_type: "total",
      ranges: [
        {
          start: "2018-01-01 00:00:00",
          end: "2018-12-31 23:00:00",
        },
      ],
    },
    {
      type: "weather_year",
      class: "WeatherYear",
      name: "weather_2018",
      description: "Weather year, 2018 only",
      file: "../../../project/dimensions/weather_years.csv",
      module: "dsgrid.dimension.standard",
    },
  ],
}