{
  dataset_id: "2025_buildings_heating_and_cooling",
  dataset_type: "modeled",
  data_source: "bss",
  sector_description: "residential_and_commercial_building_stock",
  description: "High-resolution building end-use electricity load profiles for contiguous US, breakthrough scenarios, simulated using Scout with end use load profile data from ResStock and ComStock.",
  origin_creator: "Margaret Pigman",
  origin_organization: "LBL",
  origin_contributors: [
    "Jared Langevin",
    "Jeff Deason",
    "Chioke Harris",
    "Henry Horsey",
    "Andrew Parker",
    "Handi Chandra Putra"
  ],
  origin_project: "IEF Year 2",
  origin_date: "2024-11-30",
  origin_version: "0.1.0",
  source: "LBNL, B-SS public data release, with data from ResStock (2024/resstock_amy2018_release_2) and ComStock (2023/comstock_amy2018_release_2)",
  data_classification: "low",
  tags: [
    "res",
    "com",
    "buildings",
    "Scout",
    "ResStock",
    "ComStock"
  ],
  trivial_dimensions: [
    "sector",
    "weather_year",
    "scenario",
    "subsector"
  ],
  data_schema: {
    table_format: {
      format_type: "pivoted",
      pivoted_dimension_type: "metric"
    },
    data_schema_type: "one_table"
  },
  use_project_geography_time_zone: true,
  dimensions: [
    {
      type: "geography",
      name: "State",
      display_name: "State",
      file: "../../../project/dimensions/states.csv",
      module: "dsgrid.dimension.standard",
      class: "State",
      description: "11 states in the US"
    },
    {
      type: "metric",
      name: "Buildings Heating and Cooling End Uses",
      display_name: "Buildings Heating and Cooling End Uses",
      file: "dimensions/enduses.csv",
      module: "dsgrid.dimension.standard",
      class: "EnergyEndUse",
      description: "IEF 2025 Heating and Cooling End Uses"
    },
    {
      type: "model_year",
      class: "ModelYear",
      name: "IEF 2025 Model Years",
      display_name: "Model Year",
      description: "IEF Model Years; 2030, 2040, 2050",
      file: "../../../project/dimensions/model_years.csv",
      module: "dsgrid.dimension.standard"
    },
    {
      type: "scenario",
      class: "Scenario",
      name: "Breakthrough Demand Scenario",
      display_name: "Breakthrough Demand Scenario",
      description: "Breakthrough Demand Scenario",
      file: "../../../project/dimensions/scenarios.csv",
      module: "dsgrid.dimension.standard"
    },
    {
      type: "sector",
      name: "Residential Sector",
      display_name: "Residential Sector",
      file: "../../../project/dimensions/sectors.csv",
      module: "dsgrid.dimension.standard",
      class: "Sector",
      description: "Residential Sector"
    },
    {
      type: "subsector",
      class: "Subsector",
      name: "IEF 2025 Demand Subsectors",
      display_name: "IEF 2025 Demand Subsectors",
      description: "Unspecified subsectors",
      file: "../../../project/dimensions/subsectors.csv",
      module: "dsgrid.dimension.standard"
    },
    {
      type: "weather_year",
      class: "WeatherYear",
      name: "2018 Weather Year",
      display_name: "Weather 2018",
      description: "Weather year, 2018 only",
      file: "../../../project/dimensions/weather_years.csv",
      module: "dsgrid.dimension.standard"
    },
    {
      type: "time",
      class: "Time",
      time_type: "index",
      name: "local_index_time",
      display_name: "local_index_time",
      description: "8760-indexed time with 2018-01-01 00:00:00 start. The data are laid out like standard time and have days of the week aligned with what's expected for weather year 2018, but both the data and timestamps need daylight saving adjustment to local prevailing time.",
      time_interval_type: "period_ending",
      str_format: "%Y-%m-%d %H:%M:%S",
      starting_timestamps: ["2018-01-01 00:00:00"],
      frequency: "P0DT1H0M0.000000S", 
      measurement_type: "total",
      ranges: [
        {
          start: 0,
          end: 8759
        }
      ]
    }
  ]
}
