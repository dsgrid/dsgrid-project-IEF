{
  dataset_id: "ief_2025_com_residuals",
  dataset_type: "modeled",
  sector_description: "Commercial",
  description: "Residuals for the contiguous US for IEF 2025 Stated Policies Scenario, calculated using buildings, industry (data centers and water treatment), dgen and eia861 data for 2018.",
  origin_creator: "Ashreeta Prasanna",
  origin_organization: "NREL",
  origin_contributors: [
    "Ashreeta Prasanna",
    "Elaine Hale",
    "Daniel Thom"
  ],
  origin_project: "IEF",
  origin_date: "2025-07-09",
  origin_version: "v0.1.0",
  data_source: "EIA, NREL, Scout, dGen",
  source: "NREL",
  data_classification: "low",
  tags: [
    "commercial",
    "residual"
  ],
  trivial_dimensions: ['weather_year', 'scenario','sector','subsector'],
  data_schema: {
    table_format: {
      format_type: "unpivoted",
      value_column: "value",
    },
    data_schema_type: "standard",
  },

  dimensions: [
    {
      type: "geography",
      name: "state",
      file: "../../../project/dimensions/supplemental/states.csv",
      module: "dsgrid.dimension.standard",
      class: "State",
      description: "US States",
    },
    {
      type: "metric",
      name: "unspecified_electricity",
      file: "dimensions/enduses.csv",
      module: "dsgrid.dimension.standard",
      class: "EnergyEndUse",
      description: "Unspecified end use electricity",
    },
    {
      type: "model_year",
      name: "project model_years",
      file: "dimensions/model_years.csv",
      module: "dsgrid.dimension.standard",
      class: "ModelYear",
      description: "2018 model year",
    },
    {
      type: "scenario",
      class: "Scenario",
      name: "stated_policies_scenario",
      description: "IEF 2025 stated policies scenario",
      file: "dimensions/scenarios.csv",
      module: "dsgrid.dimension.standard",
    },
    {
      type: "sector",
      name: "sector",
      file: "dimensions/sectors.csv",
      module: "dsgrid.dimension.standard",
      class: "Sector",
      description: "Residential Sector",
    },
    {
      type: "subsector",
      class: "Subsector",
      name: "unspecified_subsector",
      description: "unspecified subsectors",
      file: "dimensions/subsectors_unspecified.csv",
      module: "dsgrid.dimension.standard",
    },
    {
      type: "time",
      class: "Time",
      frequency: "P0DT1H0M0.000000S",
      name: "time_est",
      time_type: "datetime",
      leap_day_adjustment: "none",
      description: "Time dimension, 2018 hourly EST, period ending, no Leap Day Adjustment, total value",
      time_interval_type: "period_beginning",
      str_format: "%Y-%m-%d %H:%M:%S",
      timezone: "EasternStandard",
      measurement_type: "total",
      ranges: [
        {
          start: "2018-01-01 00:00:00",
          end: "2018-12-31 23:00:00",
        },
      ],
    },
    {
      type: "weather_year",
      class: "WeatherYear",
      name: "weather_2018",
      description: "Weather year, 2018 only",
      file: "dimensions/weather_years.csv",
      module: "dsgrid.dimension.standard",
    },
  ],
}
