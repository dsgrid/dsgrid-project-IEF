  datasets: [
    {
      dataset_id: 'ief_2025_buildings',
      dataset_type: 'modeled',
      version: '1.0.0',
      required_dimensions: {
        single_dimensional: {
          subsector: {
            base: [
              'unspecified',
            ],
          },
          geography: {
              base_missing: {record_ids: ['48301'], dimension_name: "county"},
          },
          model_year: {
              base_missing: {record_ids: ['2023'], dimension_name: "model_year"},

          },
        },
        multi_dimensional: [
          {
            sector: {
              base: [
                'com',
              ],
            },
            metric: {
              subset: [
                {
                  name: 'end_uses_by_sector',
                  selectors: [
                    'commercial_end_uses',
                  ],
                },
              ],
            },
          },
          {
            sector: {
              base: [
                'res',
              ],
            },
