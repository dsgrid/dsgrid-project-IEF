          fall_back_hour: "duplicate",
        },
      },
      dataset_type: 'modeled',
      version: '1.0.0',
      required_dimensions: {
        single_dimensional: {
          sector: {
            base: [
              'ind',
            ],
          },
          subsector: {
            subset: [
