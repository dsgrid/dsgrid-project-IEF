{
  project_id: 'US_DOE_IEF_2025',
  description: 'Project created to demonstrate multiple metrics in dsgrid',
  name: 'U.S. DOE IEF High Resolution Load Projections 2025',
  datasets: [
    {
      dataset_id: '2025_buildings_heating_and_cooling',
      wrap_time_allowed: true,
      dataset_type: 'modeled',
      version: '1.0.0',
      required_dimensions: {
        single_dimensional: {
          sector: {
            base: ['res'],
          },
          metric: {
            base: {
              record_ids: [
                "electricity_heating", "electricity_cooling"
              ],
              dimension_name: "Buildings Heating and Cooling End Uses",
            },
        },
    },
    {
      dataset_id: '2025_shed_hourly',
      wrap_time_allowed: true,
      dataset_type: 'modeled',
      version: '1.0.0',
      required_dimensions: {
        single_dimensional: {
          sector: {
            base: ['res'],
          },
          metric: {
            base: {
              record_ids: [
                "heating_shed_capacity", "cooling_shed_capacity"
              ],
              dimension_name: "Shed Capacity",
            },
          },
        },
    },
    {
      dataset_id: '2025_shed_annual',
      wrap_time_allowed: true,
      dataset_type: 'modeled',
      version: '1.0.0',
      required_dimensions: {
        single_dimensional: {
          sector: {
            base: ['res'],
            },
          metric: {
            base: {
              record_ids: [
                "heating_shed_capacity", "cooling_shed_capacity"
              ],
              dimension_name: "Shed Capacity",
            },
          },
          },
        },
      },
    },
    {
      dataset_id: '2025_shed_percentage',
      wrap_time_allowed: true,
      dataset_type: 'modeled',
      version: '1.0.0',
      required_dimensions: {
        single_dimensional: {
          sector: {
            base: ['res'],
          },
          metric: {
            base: {
              record_ids: [
                "shed_percentage",
              ],
              dimension_name: "Shed Percentage",
            },
        },
      },
    },
  ],

  dimensions: {
    base_dimensions: [
      {
        type: 'geography',
        class: 'State',
        name: '11 US States',
        display_name: 'State',
        description: '11 States; AZ, CA, CO, FL, GA, MN, MT, NM, NY, TX, WA'
        file: 'dimensions/states.csv',
        module: 'dsgrid.dimension.standard',
      },
      {
        type: 'sector',
        class: 'Sector',
        name: 'Sectors - Res',
        display_name: 'Sector',
        description: 'Flexibility data March 2025;  Residential heating and cooling',
        file: 'dimensions/sectors.csv',
        module: 'dsgrid.dimension.standard',
      },
      {
        type: 'metric',
        class: 'EnergyEndUse',
        name: 'Buildings Heating and Cooling End Uses',
        display_name: 'End Use',
        description: 'IEF 2025 Heating and Cooling End Uses',
        file: 'dimensions/enduses.csv',
        module: 'dsgrid.dimension.standard',
      },
      {
        type: 'metric',
        class: 'Stock',
        name: 'Capacity',
        display_name: 'Capacity',
        description: 'Shed Capacity', 
        file: 'dimensions/capacity.csv',
        module: 'dsgrid.dimension.standard',
      },
      {
        type: 'metric',
        class: 'Stock', 
        name: 'Percentage',
        display_name: 'Percentage',
        description: 'Shed Percentage', 
        file: 'dimensions/percentage.csv', 
        module: 'dsgrid.dimension.standard',
      },
      {
        type: 'model_year',
        class: 'ModelYear',
        name: 'IEF 2025 Model Years',
        display_name: 'Model Year',
        description: 'IEF 2025 Model Years; 2030, 2040, 2050',
        file: 'dimensions/model_years.csv',
        module: 'dsgrid.dimension.standard',
      },
      {
        type: 'scenario',
        class: 'Scenario',
        name: 'Breakthrough Demand Scenario',
        display_name: 'Scenario',
        description: 'Breakthrough Demand Scenario',
        file: 'dimensions/scenarios.csv',
        module: 'dsgrid.dimension.standard',
      },
      {
        type: 'time',
        class: 'Time',
        frequency: 'P0DT1H0M0.000000S',
        name: 'Time-2018-CT-hourly-periodEnding-noLeapDayAdjustment-total',
        display_name: 'Time CT',
        time_type: 'datetime',
        leap_day_adjustment: 'none',
        description: 'Time dimension, 2018 hourly EST, hour-ending, no Leap Day Adjustment, total value',
        time_interval_type: 'period_beginning',
        str_format: '%Y-%m-%d %H:%M:%S',
        timezone: 'CentralStandard',
        measurement_type: 'total',
        ranges: [
          {
            start: '2018-01-01 00:00:00',
            end: '2018-12-31 23:00:00',
          },
        ],
      },
      {
        type: 'weather_year',
        class: 'WeatherYear',
        name: '2018 Weather Year',
        display_name: 'Weather 2018',
        description: 'Weather year, 2018 only',
        file: 'dimensions/weather_years.csv',
        module: 'dsgrid.dimension.standard',
      },
    ],
    subset_dimensions: [
      {
      name: 'End Uses by Fuel Type',
      display_name: 'end_uses_by_fuel_type',
      description: 'Provides selection of end uses by fuel type.csv',
      type: 'metric',
      filename: 'dimensions/subset/end_uses_by_fuel_type.csv',
      selectors: [
        {
          name: 'electricity_end_uses',
          description: 'All Electric End Uses',
          column_values: { fuel_id: 'electricity', unit: 'MWh' },
        },
      ],
    },
    ],
    supplemental_dimensions: [
      {
        type: 'geography',
        class: 'GeographyDimensionBaseModel',
        name: 'ReEDS PCA',
        display_name: 'ReEDS PCA',
        description: 'ReEDS Power Control Areas',
        file: 'dimensions/supplemental/reeds_pca.csv',
        module: 'dsgrid.dimension.standard',
        mapping: {
          description: 'Maps 11 US States to ReEDS Power Control Areas',
          file: 'dimension_mappings/lookup_state_to_reeds_pca.csv',
          mapping_type: 'many_to_one_aggregation',
        },
      },
      {
        class: 'ModelYear',
        description: 'Interpolated model years',
        file: 'dimensions/supplemental/all_years.csv',
        module: 'dsgrid.dimension.standard',
        name: 'All years',
        display_name: 'All years',
        type: 'model_year',
        mapping: {
          description: 'model years interpolated to all years',
          file: 'dimension_mappings/lookup_all_years_to_model_years.csv',
          mapping_type: 'many_to_many_explicit_multipliers',
      },
      },
      },
    ],
  },
}
